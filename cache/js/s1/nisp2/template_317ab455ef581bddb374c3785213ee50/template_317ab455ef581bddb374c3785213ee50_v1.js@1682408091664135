$('body').on('click', '.top-catalog-nav .nav-item', function () {
    var id = $(this).data('id')
    if ($(this).hasClass('active') && $(this).closest('.site-header').hasClass('fixed')) {
        $(this).removeClass('active')
        $('.top-catalog-menu-wrap').removeClass('open')
        return;
    }
    $('.top-catalog-nav .nav-item').removeClass('active')
    $(this).addClass('active')
    $('.top-catalog-menu-wrap').removeClass('open')
    $('#'+id).addClass('open')
})
/** Hover для меню каталога в шапке на десктопе */
$('body').on('mouseover', '.top-catalog-nav .nav-item', function () {
    // console.log($(this).closest('.site-header'))
    if ($(this).closest('.site-header').hasClass('fixed')) {
        return;
    }
    var id = $(this).data('id')
    $('.top-catalog-nav .nav-item').removeClass('active')
    $(this).addClass('active')
    $('.top-catalog-menu-wrap').removeClass('open')
    $('#'+id).addClass('open')
})

$('body').on('mouseover', function (e) {
    var target = $(e.target)
    if (
        !target.hasClass('main-line') 
        && !target.hasClass('top-catalog-nav')
        && target.closest('top-catalog-nav').length == 0
        && !target.hasClass('top-catalog-menu-wrap') 
        && target.closest('.top-catalog-menu-wrap').length == 0
        && !target.hasClass('.nav-item')
        && target.closest('.nav-item').length == 0
    ) {
        $('.top-catalog-menu-wrap').removeClass('open')
        $('.top-catalog-nav .nav-item').removeClass('active')
    }
})

$('body').on('click', '.top-catalog-menu .menu-more', function (e) {
    var parentWrap = $(this).closest('.subsection-list')

    if (parentWrap.hasClass('show')) {
        parentWrap.removeClass('show')
        $(this).find('.title').text('Развернуть')
    } else {
        parentWrap.addClass('show')
        $(this).find('.title').text('Свернуть')
    }
})
$('body').on({
    mouseenter: function () {
        if (!$(this).data('id')) {
            return;
        }
        var itemId = $(this).data('id'),
            dataUrl = $(this).attr('data-img'),
            _self = $(this),
            src = null


            // console.log($(this))
        if (dataUrl.length == 0) {
            $.ajax({
                method: 'post',
                url: '/ajax/catalogmenu.php',
                dataType: 'json',
                data: {action: 'getImage', id: itemId},
                success: function (resp) {
                    if (resp.url) {
                        _self.attr('data-img', resp.url)
                        src = resp.url
                        $('.top-catalog-item-image').html('<img src="'+src+'">')
                    }
                }
            })
        } else {
            src = dataUrl
            $('.top-catalog-item-image').html('<img src="'+src+'">')
        }
    },
    mouseleave: function () {
        $('.top-catalog-item-image').html('')
    }
}, '.top-catalog-menu .menu-link')
/* End */
;
; /* Start:"a:4:{s:4:"full";s:53:"/bitrix/templates/nisp2/js/element.js?168225259048394";s:6:"source";s:37:"/bitrix/templates/nisp2/js/element.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/

var catalogElementInit = function () {
    $('.element-select_list-item.active').each(function (i, el) {
        setActiveDecor($(el))
    })

    $('.element-material_item.active').each(function (i, el) {
        setActiveMaterial($(el))
    })

    if (typeof jsOffers != "undefined") {

        $('.stela-h-val-input').first().trigger('click').trigger('change')

        // вот такой гемор нужен потому что есть SKU памятников и простые SKU
        $('.element-sku_item.stone-sku').first().each(function (i, el) {
            setActiveStoneSku($(el))
            // console.log($(el))
            return;
        })
        $('.element-sku_item.stone-sku').first().each(function (i, el) {
            filterSku($(el))
            return;
        })

        $('.element-sku_item.glass-sku').first().each(function (i, el) {
            setActiveGlassSku($(el))
            return;
        })
        $('.element-sku_item.keram-sku').first().each(function (i, el) {
            setActiveKeramSku($(el))
            return;
        })

        $('.element-sku_item.simple-sku').first().each(function (i, el) {
            setActiveSimpleSku($(el))
            return;
        })
    }
    if ($('.element-install_item').first().length) {
        setActiveInstall($('.element-install_item').first())
    }
    if ($('.element-beton_item').first().length) {
        setActiveBeton($('.element-beton_item').first())
    }
    if ($('.element-radio_item').first().length) {
        setActiveRadio($('.element-radio_item').first())
    }

    $('.element-discount-item').each(function (i, el) {
        setActiveDiscount($(el))
    })

    if ($('.catalog-element').length) {
        buildChoice()
    }
}

/** сокращенный вид выбора ТП -- start */
// высота стелы
$('body').on('change', '.stela-h-val-input', function () {
    var stelaH = $(this).val()
    highlightPropVals('stela', 'h', stelaH)
})

// ширина стелы
$('body').on('change', '.stela-w-val-input', function () {
    var stelaW = $(this).val()
    highlightPropVals('stela', 'w', stelaW)
})

// толщина стелы
$('body').on('change', '.stela-t-val-input', function () {
    var stelaT = $(this).val(),
        stelaH = $('.stela-h-val-input:checked').val(),
        stelaW = $('.stela-w-val-input:checked').val()

        // установить активное sku стелы
    $('.sku-item-stela_'+stelaH+'_'+stelaW+'_'+stelaT).trigger('click').trigger('change')

    highlightTumbaVals()
})


    
// })

/* кнопки выбора тумбы отключены по просьбе заказчика */
// высота тумбы клик вручную
$('body').on('click', '.tumba-h-val-label', function (e) {
    var tumbaH = $(this).closest('.sku-val-item').find('input').val(),
    tumbaW = $('.tumba-w-val-input:checked').val(),
    tumbaT = $('.tumba-t-val-input:checked').val()
    
    $('.sku-item-tumba_'+tumbaH+'_'+tumbaW+'_'+tumbaT).trigger('click').trigger('change')
    // e.stopImmediatePropagation()
})
// ширина тумбы клик вручную
$('body').on('click', '.tumba-w-val-label', function (e) {
    // e.stopImmediatePropagation()
    $('.tumba-w-val-input').attr('checked', false)
    var tumbaW = $(this).closest('.sku-val-item').find('input').val(),
        tumbaH = $('.tumba-h-val-input:checked').val()

    $(this).closest('.sku-val-item').find('input').attr('checked', true)

    $('.tumba-t-val-input').attr('disabled', true)
    $('.sku-item-tumba:not(.d-none)').each((ind, el) => {
        if($(el).data('w') == tumbaW && $(el).data('h') == tumbaH) {
            var availT = $(el).data('t')
            $('#tumba_t_'+availT).attr('disabled', false)
        }
    })
    $('.tumba-t-val-input:not(:disabled)').first().next('label').trigger('click')
    var tumbaT = $('.tumba-t-val-input:not(:disabled)').first().val()
    $('.sku-item-tumba_'+tumbaH+'_'+tumbaW+'_'+tumbaT).trigger('click').trigger('change')
})
// толщина тумбы клик вручную
$('body').on('click', '.tumba-t-val-label', function (e) {
    var tumbaT = $(this).closest('.sku-val-item').find('input').val(),
    tumbaH = $('.tumba-h-val-input:checked').val(),
    tumbaW = $('.tumba-w-val-input:checked').val()
    console.log(tumbaH, tumbaW, tumbaT)
    
    $('.sku-item-tumba_'+tumbaH+'_'+tumbaW+'_'+tumbaT).trigger('click').trigger('change')
})


// выбрано sku тумбы
$('body').on('change', '.sku-item-tumba input', function () {
    var tumbaSkuItem = $(this).closest('.sku-item-tumba'),
        tumbaH = tumbaSkuItem.data('h'),
        tumbaW = tumbaSkuItem.data('w'),
        tumbaT = tumbaSkuItem.data('t')

    $('#tumba_h_'+tumbaH).trigger('click').trigger('change')
    $('#tumba_w_'+tumbaW).trigger('click').trigger('change')
    $('#tumba_t_'+tumbaT).trigger('click').trigger('change')
    $(this).trigger("tumbasku:select")
})
$('body').on('tumbasku:select', '.sku-item-tumba input', function () {
    // highlightTumbaVals()
})

function highlightTumbaVals () {
    // подсветить доступные размеры тумбы
    $('.tumba-h-val-input').attr('disabled', true)
    $('.tumba-w-val-input').attr('disabled', true)
    $('.tumba-t-val-input').attr('disabled', true)
    $('.sku-item-tumba:not(.d-none)').each((ind, el) => {
        var hVal = $(el).data('h'), wVal = $(el).data('w'), tVal = $(el).data('t')
        $('#tumba_h_'+hVal).attr('disabled', false)
        $('#tumba_w_'+wVal).attr('disabled', false)
        $('#tumba_t_'+tVal).attr('disabled', false)
    })
    console.log('highlight tumba vals')
}

function highlightPropVals(propCode, valCode, val) {
    if (propCode == 'stela') {
        var hl = obstelaSizesVals[valCode][val]
    } else if (propCode == 'tumba') {
        var hl = obtumbaSizesVals[valCode][val]
    }

    if (typeof hl['w'] != "undefined" && hl['w'].length > 0) {
        $('.'+propCode+'-w-val-input').attr('disabled', true).attr('checked', false)
        hl['w'].forEach((el) => {
            $('#'+propCode+'_w_'+el).attr('disabled', false)
        })
        $('.'+propCode+'-w-val-input:not(:disabled)').first().trigger('click').trigger('change')
        // return;
    }
    if (typeof hl['t'] != "undefined" && hl['t'].length > 0) {
        $('.'+propCode+'-t-val-input').attr('disabled', true).attr('checked', false)
        hl['t'].forEach((el) => {
            $('#'+propCode+'_t_'+el).attr('disabled', false)
        })
        $('.'+propCode+'-t-val-input:not(:disabled)').first().trigger('click').trigger('change')
        // return;
    }
}

/* CATALOG ELEMENT */
$(document).ready(function () {

    // on document load
    $('body').on('click', '.element-select_field', function () {
        if ($(this).hasClass('disabled')) {
            // console.log('disabled')
            return false
        }
        var elementSelect = $(this).closest('.element-select')
        if (elementSelect.hasClass('open')) {
            closeSelect()
        } else {
            elementSelect.addClass('open')
            $('body').addClass('element-select-open')
            $('body').prepend('<div class="element-select-overflow"></div>')
        }
    })

    $('body').on('click', '.element-select-overflow', function () {
        closeSelect()
    })

    $('body').on('click', '.element-select_list-item', function () {
        $(this).closest('.element-select').find('.element-select_list-item').removeClass('active')
        $(this).addClass('active')

        if ($(this).closest('.element-decor_row').length > 0) {
            $(this).closest('.element-decor_row').find('.element-decor_checkbox').attr('checked', true)
        }
        setActiveDecor($(this))
        closeSelect()
    })
    $('body').on('click', '.element-decor_checkbox', function () {
        var select = $(this).closest('.element-decor_row').find('.element-select_field')
        // if ($(this).is(':checked')) {
        //     select.removeClass('disabled')
        // } else {
        //     select.addClass('disabled')
        // }
    })

    $('body').on('click', '.element-material_item', function () {
        $('.element-material_item').removeClass('active')
        $(this).addClass('active')
        setActiveMaterial($(this))
        closeSelect()
    })

    $('body').on('click', '.susal-select-item', function () {
        setActiveSusalSelect($(this))
    })

    $('body').on('click', '.element-sku_item', function () {

        if ($(this).hasClass('stone-sku')) {
            setActiveStoneSku($(this))
            filterSku($(this))
            
        } else if ($(this).hasClass('glass-sku')) {
            setActiveGlassSku($(this))
            filterGlassSku($(this))

        } else if ($(this).hasClass('keram-sku')) {
            setActiveKeramSku($(this))
            filterKeramSku($(this))

        } else if ($(this).hasClass('simple-sku')) {
            setActiveSimpleSku($(this))
        } 
    })

    $('body').on('click', '.element-install_item', function () {
        setActiveInstall($(this))
    })
    $('body').on('click', '.element-beton_item', function () {
        setActiveBeton($(this))
    })
    $('body').on('click', '.element-radio_item', function () {
        setActiveRadio($(this))
    })

    $('body').on('click', '.element-decor_checkbox', function () {
        checkDecor($(this))
    })

    $('body').on('click', '.element-discount-item', function () {
        setActiveDiscount($(this))
    })

    $('body').on('click', '.catalog-element_photo', function () {
        $('.element-photo_popup-wrap').addClass('open')
    })
    $('body').on('click', '.element-photo_close', function () {
        $('.element-photo_popup-wrap').removeClass('open')
    })
    $('body').on('click', '.element-photo_popup-wrap', function (e) {
        if ($(e.target).hasClass('element-photo_popup-wrap')) {
            $('.element-photo_popup-wrap').removeClass('open')
        }
    })

    $('body').on('click', '.js-tofavorive', function (e) {
        e.preventDefault()
        var _self = $(this)
        _self.attr('disabled', true)
        $.ajax({
            method: 'post',
            url: '/ajax/favorite.php',
            dataType: 'json',
            data: { action: 'addRemove', id: _self.data('id') },
            success: function (resp) {
                // console.log(resp)
                if (resp.action == 'add') {
                    _self.addClass('active')
                } else {
                    _self.removeClass('active')
                }
                _self.attr('disabled', false)
                topFavoriteCount()
            }
        })
    })

    $('body').on('click', '.js-calculate-fence', function (e) {
        e.preventDefault()
        calculateFence()
    })

    $('body').on('click', '.js-calculate-area', function (e) {
        e.preventDefault()
        var isMetr = false
        if ($(this).data('metr') == 1) {
            isMetr = true
        }
        calculateArea(isMetr)
    })

    $('body').on('click', '.element-calculator .form-input', function (e) {
        if ($(this).val() == '0') {
            $(this).val('')
        }
    })

    $('body').on('click', '.element-photoprint_item', function (e) {
        if (!cartChoice.areaPrice) {
            return false;
        }
        setActivePhotoprintService($(this))
    })

    $('body').on('click', '.detail-request-button', function (e) {
        e.preventDefault()
        $(this).addClass('d-none')
        $('.request-price-form').removeClass('d-none')
    })
    $('body').on('submit', '.request-price-form', function (e) {
        e.preventDefault()
        var _self = $(this)
        $.ajax({
            method: 'post',
            url: '/ajax/callback.php',
            dataType: 'json',
            data: _self.serialize(),
            success: function (resp) {
                _self.addClass('d-none')
                $('.request-price-result').removeClass('d-none')
            }
        })
    })

}) // document ready


var decorSetCheck = function (target) {
    
}

var checkDecor = function (target) {
    var activeDecorItem = target.closest('.element-decor_row').find('.element-select_list-item.active')
    if (target.is(':checked')) {
        setActiveDecor(activeDecorItem)
    } else {
        delete cartChoice.props[activeDecorItem.data('code')]
        delete cartChoice.prices[activeDecorItem.data('code')]

        if (activeDecorItem.data('install') && cartChoice.pMetr) {
            var installBlock = $('.element-install_item.install-yes'),
                summInstall = cartChoice.fenceInstall

            installBlock.attr('data-price', summInstall)
                .find('.catalog-install_value').text(BX.Currency.currencyFormat(summInstall, "RUB", 1))
            if (cartChoice.prices['install']) {
                cartChoice.prices['install'] = summInstall
                cartChoice.props['install']['price'] = summInstall
            }
        }
    }
    calculatePrice()
    buildChoice()
}

var setActiveDecor = function (target) {
    var selName = target.attr('data-name'),
        selPrice = !target.hasClass('element-letter_item') ? parseInt(target.attr('data-price')) : ''
    var itemPrice = parseInt(target.attr('data-price')),
        printPrice = ''

    if (selPrice) {
        printPrice = ' - ' + BX.Currency.currencyFormat(selPrice, "RUB", 1)
    }

    target.closest('.element-select').find('.element-select_title').text(selName + printPrice)

    if (target.closest('.element-decor_row').find('.element-decor_checkbox').is(':checked')) {
        cartChoice.props[target.data('code')] = { title: target.data('title'), value: target.data('value'), price: itemPrice, code: target.data('code') }
        cartChoice.prices[target.data('code')] = itemPrice

        if (target.data('install') && cartChoice.pMetr) {
            var installBlock = $('.element-install_item.install-yes'),
                // inslallOne = installBlock.data('priceone'),
                activeDecorInstall = parseInt(target.data('install')) * cartChoice.pMetr,
                summInstall = activeDecorInstall + cartChoice.fenceInstall

            installBlock.attr('data-price', summInstall)
                .find('.catalog-install_value').text(BX.Currency.currencyFormat(summInstall, "RUB", 1))
            if (cartChoice.prices['install']) {
                cartChoice.prices['install'] = summInstall
                cartChoice.props['install']['price'] = summInstall
            }
        }

    }

    if (target.hasClass('element-letter_item')) {
        calculateLetters()
    }
    
    calculatePrice()
    buildChoice()
}

var setActiveSusalSelect = function (target) {
    // console.log(target.data('price'))

    cartChoice.letters.upperPrice = parseInt(target.data('price'))
    cartChoice.letters.lowerPrice = parseInt(target.data('price'))
    cartChoice.letters.digitPrice = parseInt(target.data('price'))
    cartChoice.letters.symbolsPrice = parseInt(target.data('price'))
    calculateLetters()
    calculatePrice()
    buildChoice()
}

var setActiveInstall = function (target) {
    $('.element-install_item').removeClass('active')
    target.addClass('active')
    $('.element-install_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    cartChoice.props[target.data('code')] = { title: target.data('title'), value: target.data('value'), price: target.data('price'), code: target.data('code') }
    cartChoice.prices[target.data('code')] = target.data('price')
    calculatePrice()
    buildChoice()
}
var setActiveBeton = function (target) {
    $('.element-beton_item').removeClass('active')
    target.addClass('active')
    $('.element-beton_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    cartChoice.props[target.data('code')] = { title: target.data('title'), value: target.data('value'), price: target.data('price'), code: target.data('code') }
    cartChoice.prices[target.data('code')] = target.data('price')
    calculatePrice()
    buildChoice()
}

var setActiveRadio = function (target) {
    var targetRadioParent = target.closest('.element-radio')
    targetRadioParent.find('.element-radio_item').removeClass('active')
    target.addClass('active')
    targetRadioParent.find('.custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    // золотые буквы
    if(target.hasClass('gold-method-item')) {
        console.log(target.find('.gold-method-input').val())
        var methodCode = target.find('.gold-method-input').val()
        if (methodCode == 'susal') {
            setActiveSusalSelect($('.susal-select-item').first())
            $('.susal-select').removeClass('d-none')
        } else {
            cartChoice.letters.upperPrice = parseInt(target.data('letterprice'))
            cartChoice.letters.lowerPrice = parseInt(target.data('letterprice'))
            cartChoice.letters.digitPrice = parseInt(target.data('letterprice'))
            cartChoice.letters.symbolsPrice = parseInt(target.data('signprice'))
            calculateLetters()
            $('.susal-select').addClass('d-none')
        }
    }

    // скарпель
    if(target.hasClass('skarpel-method-item')) {
        console.log(target.find('.skarpel-method-input').val())
        var methodCode = target.find('.skarpel-method-input').val()
        
        cartChoice.letters.upperPrice = parseInt(target.data('letterprice'))
        cartChoice.letters.lowerPrice = parseInt(target.data('letterprice'))
        cartChoice.letters.digitPrice = parseInt(target.data('letterprice'))
        cartChoice.letters.symbolsPrice = parseInt(target.data('letterprice'))
        calculateLetters()
    }

    cartChoice.props[target.data('code')] = { 
        title: target.data('title'), 
        value: target.data('value'), 
        price: target.data('price'), 
        code: target.data('code') 
    }
    cartChoice.prices[target.data('code')] = target.data('price')
    calculatePrice()
    buildChoice()
}

var calculateSkuInstall = function () {
    var installPrice = 0
    $('.element-sku_item.active').each(function (i, el) {
        installPrice += parseInt($(el).attr('data-install'))
    })
    var formatPrice = BX.Currency.currencyFormat(installPrice, "RUB", 1)
    $('.element-install_item.install-yes').attr('data-price', installPrice).find('.catalog-install_value').text(formatPrice)
    if (cartChoice.prices.install) {
        // console.log(cartChoice.props.install)
        cartChoice.props.install.price = installPrice
        cartChoice.prices.install = installPrice
    }
}

var calculateSkuWeight = function () {
    var weight = 0
    $('.element-sku_item.active').each(function (i, el) {
        weight += parseInt($(el).attr('data-weight'))
    })
    cartChoice.weight = weight
    if (weight > 0) {
        $('.element-weight').show()
        $('.element-weight .value').text(weight + ' кг')
    } else {
        $('.element-weight').hide()
    }
}

var setActiveMaterial = function (target) {
    var selName = target.data('name'),
        selRatio = target.data('ratio'),
        selImg = target.data('image')

    target.closest('.element-select').find('.element-select_title').text(selName)
    $('.element-material_current').css('background-image', 'url(' + selImg + ')')
    // cartChoice.prices['material'] = target.data('price')
    cartChoice.props['material'] = { title: target.data('title'), value: target.data('value'), code: 'material', price: null }
    cartChoice.priceRatio = selRatio
    calculatePriceRatio()
    calculatePrice()
    buildChoice()
}

var closeSelect = function () {
    $('.element-select').removeClass('open')
    $('body').removeClass('element-select-open')
    $('.element-select-overflow').remove()
}

var setActiveStoneSku = function (target) {
    var currentBlock = target.closest('.element-sku')
    currentBlock.find('.element-sku_item').removeClass('active')
    target.addClass('active')
    currentBlock.find('.element-sku_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true).trigger('change')

    if (target.data('price')) {
        if (target.data('pmstela')) {
            cartChoice.pmstela = target.data('pmstela')

            // пересчет цены фаски в памятниках
            $('.element-faska_item').each(function (i, el) {
                var pmStela = parseFloat(target.data('pmstela')),
                    faskaPriceOne = parseInt($(el).data('priceone')),
                    faskaPrice = pmStela * faskaPriceOne

                $(el).attr('data-price', faskaPrice)
                    .find('.element-price').text(BX.Currency.currencyFormat(faskaPrice, "RUB", 1))
            })
            setActiveDecor($('.element-faska_item.active'))

            if (cartChoice.prices['faska']) {
                var activeItem = $('.element-faska_item.active'),
                    activeItemPrice = parseFloat(activeItem.attr('data-price'))

                cartChoice.prices['faska'] = activeItemPrice
                cartChoice.props['faska']['price'] = activeItemPrice
            }
        }

        cartChoice.prices[target.data('code')] = target.data('price')
        cartChoice.props[target.data('code')] = { title: target.data('title'), value: target.data('value'), price: target.data('price'), code: target.data('code') }
    } else {
        delete cartChoice.prices[target.data('code')]
        delete cartChoice.props[target.data('code')]
    }

    calculateSkuInstall()
    calculateSkuWeight()
    calculatePrice()
    buildChoice()
}

var setActiveGlassSku = function (target) {
    var currentBlock = target.closest('.element-sku')
    currentBlock.find('.element-sku_item').removeClass('active')
    target.addClass('active')
    currentBlock.find('.element-sku_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    filterGlassSku(target)
    calculatePrice()
    buildChoice()
}

var setActiveKeramSku = function (target) {
    var currentBlock = target.closest('.element-sku')
    currentBlock.find('.element-sku_item').removeClass('active')
    target.addClass('active')
    currentBlock.find('.element-sku_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    filterKeramSku(target)
    calculatePrice()
    buildChoice()
}

var setActiveSimpleSku = function (target) {

    var currentBlock = target.closest('.element-sku')
    currentBlock.find('.element-sku_item').removeClass('active')
    target.addClass('active')
    currentBlock.find('.element-sku_item .custom-control-input').attr('checked', false)
    target.find('.custom-control-input').attr('checked', true)

    // cartChoice.prices.product = target.data('price')
    // cartChoice.productPrice = target.data('price')
    cartChoice.props['skuItem'] = {
        title: 'Предложение',
        value: target.data('value'),
        code: 'skuItem',
        price: target.data('price')
    }
    cartChoice.prices['skuItem'] = target.data('price')

    // calculateSkuInstall()

    if (target.hasClass('sku-pmetr')) {
        calculateFence()
    }
    var skuInstall = parseInt(target.data('install')),
        installBlock = $('.element-install_item.install-yes')

    installBlock.attr('data-priceone', skuInstall).attr('data-price', skuInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(skuInstall, "RUB", 1))
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = skuInstall
        cartChoice.props['install']['price'] = skuInstall
    }

    calculateSkuWeight()
    calculatePrice()
    buildChoice()
}

var filterSku = function (target) {
    var arProp = target.data('treevalue').split('_'),
        propId = arProp[0],
        propVal = arProp[1],
        currentBlock = target.closest('.element-sku')

    // console.log(propId, jsOffers)
    var filterSku = jsOffers.filter(function (it) {
        return it.TREE['PROP_' + propId] == propVal
    })

    var arIds = []
    for (var i = 0; i < filterSku.length; i++) {

        for (var key in filterSku[i].TREE) {
            if (key.split("_")[1] != propId) {
                arIds.push('sku_' + key.toLowerCase() + "_" + filterSku[i].TREE[key])
            }
        }
    }

    var uniqueIds = arIds.filter(function (item, pos, self) {
        return self.indexOf(item) == pos;
    })
    //
    var arTumbaValues = {}
    var stelaValue = $('.sku-item-stela.active').attr('title')

    $('.element-sku').each(function (ind, el) {
        if ($(el).data('order') <= currentBlock.data('order')) {
            return;
        }
        var elItems = $(el).find('.element-sku_item')

        elItems.removeClass('visible d-none')
        for (var i = 0; i < uniqueIds.length; i++) {
            $("#" + uniqueIds[i]).addClass('visible')
            //
            if($("#" + uniqueIds[i]).data('code') == 'tumba') {
                arTumbaValues[uniqueIds[i]] = $("#" + uniqueIds[i]).attr('title')

                /**/
                tumbaValue = $("#" + uniqueIds[i]).attr('title')
                var findOffer = arOffers.filter(function (item) {
                    return (item.PROPERTIES.STELA.VALUE == stelaValue && item.PROPERTIES.TUMBA.VALUE == tumbaValue)
                })
                $('#'+uniqueIds[i]).attr('data-install', findOffer[0].PROPERTIES.INSTALL_TUMBA.VALUE)
            }
        }

        elItems.each( function (i, el) {
            if (!$(el).hasClass('visible') && !$(el).hasClass('nothide')) {
                $(el).addClass('d-none')
            }
        })

        var firstSkuItem = $(el).find('.element-sku_item:not(.d-none)').first()
        setActiveStoneSku(firstSkuItem)
        if (firstSkuItem.hasClass('sku-item-tumba')) {
            highlightTumbaVals()
        }
    })
    //
    // var stelaValue = $('.sku-item-stela.active').attr('title')
    // for (var id in arTumbaValues) {
    //     var findOffer = arOffers.filter(function (item) {
    //         return (item.PROPERTIES.STELA.VALUE == stelaValue && item.PROPERTIES.TUMBA.VALUE == arTumbaValues[id])
    //     })
    //     $('#'+id).attr('data-install', findOffer[0].PROPERTIES.INSTALL_TUMBA.VALUE)
    //     console.log(id, findOffer[0].PROPERTIES.INSTALL_TUMBA.VALUE)
    // }
    // console.log(stelaValue, arTumbaValues)
}

// фото на стелке
var filterGlassSku = function (target) {
    var arProp = target.data('treevalue').split('_'),
        propId = arProp[0],
        propVal = arProp[1],
        currentBlock = target.closest('.element-sku')

    var filterSku = jsOffers.filter(function (it) {
        return it.TREE['PROP_' + propId] == propVal
    })

    var arIds = []
    for (var i = 0; i < filterSku.length; i++) {

        for (var key in filterSku[i].TREE) {
            if (key.split("_")[1] != propId) {
                arIds.push('sku_' + key.toLowerCase() + "_" + filterSku[i].TREE[key])
            }
        }
    }

    var uniqueIds = arIds.filter(function (item, pos, self) {
        return self.indexOf(item) == pos;
    })

    // var arSizeValues = {}
    // var kromkaValue = $('.sku-item-kromka.active').attr('title')
    var findOffer = null
    $('.element-sku').each(function (ind, el) {
        if ($(el).data('order') <= currentBlock.data('order')) {
            return;
        }
        var elItems = $(el).find('.element-sku_item')

        elItems.removeClass('visible d-none')
        for (var i = 0; i < uniqueIds.length; i++) {
            $("#" + uniqueIds[i]).addClass('visible')
        }

        elItems.each( function (i, el) {
            if (!$(el).hasClass('visible') && !$(el).hasClass('nothide')) {
                $(el).addClass('d-none')
            }
        })
        setActiveGlassSku($(el).find('.element-sku_item:not(.d-none)').first())
    })

    // console.log(arOffers[0].PROPERTIES.KROMKA.VALUE_ENUM_ID)

    var propOneValue = $('.sku-item-kromka.active').data('onevalue'),
        propTwoValue = $('.sku-item-glass_size.active').data('onevalue')

    findOffer = arOffers.filter(function (item) {
        return (item.PROPERTIES.KROMKA.VALUE_ENUM_ID == propOneValue && item.PROPERTIES.GLASS_SIZE.VALUE_ENUM_ID == propTwoValue)
    })

    // console.log(findOffer)

    cartChoice.prices['skuItem'] = parseFloat(findOffer[0].CATALOG_PRICE_1)
    cartChoice.props['skuItem'] = { 
        title: 'Предложение', 
        value: findOffer[0].NAME + '<br>Кромка: '+ findOffer[0].PROPERTIES.KROMKA.VALUE + '<br>Размер: '+findOffer[0].PROPERTIES.GLASS_SIZE.VALUE, 
        price: parseFloat(findOffer[0].CATALOG_PRICE_1), 
        code: 'skuItem'
    }

    var boltInstall = findOffer[0].PROPERTIES.INSTALL_BOLT.VALUE,
        nicheInstall = findOffer[0].PROPERTIES.INSTALL_NICHE.VALUE,
        nicheStandartInstall = findOffer[0].PROPERTIES.INSTALL_ST_NICHE.VALUE,
        nicheDeepInstall = findOffer[0].PROPERTIES.INSTALL_DEEP_NICHE.VALUE,
        activeInstall = $('.element-install_item.active').attr('data-price')

    $('.install-install_bolt').attr('data-price', boltInstall).attr('data-priceone', boltInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(boltInstall, "RUB", 1))
    $('.install-install_niche').attr('data-price', nicheInstall).attr('data-priceone', nicheInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(nicheInstall, "RUB", 1))
    $('.install-install_st_niche').attr('data-price', nicheStandartInstall).attr('data-priceone', nicheStandartInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(nicheStandartInstall, "RUB", 1))
    $('.install-install_deep_niche').attr('data-price', nicheDeepInstall).attr('data-priceone', nicheDeepInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(nicheDeepInstall, "RUB", 1))
    
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = parseFloat(activeInstall)
        cartChoice.props['install']['price'] = parseFloat(activeInstall)
    }
    calculatePrice()
    calculateSkuWeight()
    buildChoice()
}

// фото на керамике
var filterKeramSku = function (target) {
    var arProp = target.data('treevalue').split('_'),
        propId = arProp[0],
        propVal = arProp[1],
        currentBlock = target.closest('.element-sku')

    var filterSku = jsOffers.filter(function (it) {
        return it.TREE['PROP_' + propId] == propVal
    })

    var arIds = []
    for (var i = 0; i < filterSku.length; i++) {

        for (var key in filterSku[i].TREE) {
            if (key.split("_")[1] != propId) {
                arIds.push('sku_' + key.toLowerCase() + "_" + filterSku[i].TREE[key])
            }
        }
    }

    var uniqueIds = arIds.filter(function (item, pos, self) {
        return self.indexOf(item) == pos;
    })

    // var arSizeValues = {}
    // var kromkaValue = $('.sku-item-kromka.active').attr('title')
    var findOffer = null
    $('.element-sku').each(function (ind, el) {
        if ($(el).data('order') <= currentBlock.data('order')) {
            return;
        }
        var elItems = $(el).find('.element-sku_item')

        elItems.removeClass('visible d-none')
        for (var i = 0; i < uniqueIds.length; i++) {
            $("#" + uniqueIds[i]).addClass('visible')
        }

        elItems.each( function (i, el) {
            if (!$(el).hasClass('visible') && !$(el).hasClass('nothide')) {
                $(el).addClass('d-none')
            }
        })
        setActiveKeramSku($(el).find('.element-sku_item:not(.d-none)').first())
    })

    var propOneValue = $('.sku-item-material.active').data('onevalue'),
        propTwoValue = $('.sku-item-keram_size.active').data('onevalue')

    findOffer = arOffers.filter(function (item) {
        return (item.PROPERTIES.MATERIAL.VALUE_ENUM_ID == propOneValue && item.PROPERTIES.KERAM_SIZE.VALUE_ENUM_ID == propTwoValue)
    })

    // console.log(findOffer)

    cartChoice.prices['skuItem'] = parseFloat(findOffer[0].CATALOG_PRICE_1)
    cartChoice.props['skuItem'] = { 
        title: 'Предложение', 
        value: findOffer[0].NAME + '<br>Материал: '+ findOffer[0].PROPERTIES.MATERIAL.VALUE + '<br>Размер: '+findOffer[0].PROPERTIES.KERAM_SIZE.VALUE, 
        price: parseFloat(findOffer[0].CATALOG_PRICE_1), 
        code: 'skuItem'
    }

    var nicheStandartInstall = findOffer[0].PROPERTIES.INSTALL_ST_NICHE.VALUE,
        nicheDeepInstall = findOffer[0].PROPERTIES.INSTALL_DEEP_NICHE.VALUE,
        activeInstall = $('.element-install_item.active').attr('data-price')

    $('.install-install_st_niche').attr('data-price', nicheStandartInstall).attr('data-priceone', nicheStandartInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(nicheStandartInstall, "RUB", 1))
    $('.install-install_deep_niche').attr('data-price', nicheDeepInstall).attr('data-priceone', nicheDeepInstall)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(nicheDeepInstall, "RUB", 1))
    
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = parseFloat(activeInstall)
        cartChoice.props['install']['price'] = parseFloat(activeInstall)
    }
    calculatePrice()
    calculateSkuWeight()
    buildChoice()
}

var calculatePriceRatio = function () {
    // console.log('ratio='+cartChoice.priceRatio)
    var skuItems = $('.element-sku_item')
    skuItems.each(function (ind, el) {
        var itemPrice = $(el).attr('data-price'),
            calcPrice = parseFloat(itemPrice) * cartChoice.priceRatio,
            formated = BX.Currency.currencyFormat(calcPrice, "RUB", 1)
        $(el).find('.price-col').text(formated)
    })
}

var calculatePrice = function () {  
    //суммируем из массива cartChoice.prices
    var result = Object.values(cartChoice.prices).reduce(function (sum, current) {
        return sum + current;
    }, 0);
    cartChoice.pricesSumm = result
    cartChoice.price = cartChoice.pricesSumm * cartChoice.priceRatio
    cartChoice.priceFinal = cartChoice.price
    calculateDiscount()
}

var buildChoice = function () {

    var listDiv = $('.element-choice-list')
    listDiv.empty()

    // console.log(BX.Currency.currencyFormat(cartChoice.price, "RUB", 1))

    for (ind in cartChoice.props) {
        var propItem = cartChoice.props[ind],
            title = propItem.title, value = propItem.value,
            price = propItem.price ? BX.Currency.currencyFormat(propItem.price*cartChoice.priceRatio, "RUB", 1) : ''

        if (!value) {
            continue;
        }
        // console.log(value, price)
        var listItem = '<div class="element-choice-list_item d-flex d-lg-block d-xl-flex align-items-end justify-content-between">'
            + '<div class=""><div class="label">' + title + '</div><div class="value">' + value + '</div></div>'
            + '<div class="" style="min-width:70px"><div class="price">' + price + '</div></div>'
            + '</div>'
        listDiv.append(listItem)
    }
    $('.element-choice-summ').text(BX.Currency.currencyFormat(cartChoice.price, "RUB", 1))

    if (!cartChoice.price && !cartChoice.buyZeroPrice) {
        $('.to-cart-button').addClass('disabled')
    } else {
        $('.to-cart-button').removeClass('disabled')
    }
    showRequestPrice()
}

var showRequestPrice = function () {
    if (!cartChoice.price && cartChoice.useRequestPrice) {
        $('.detail-buy-block').addClass('d-none')
        $('.detail-request-block').removeClass('d-none')
    } else {
        $('.detail-buy-block').removeClass('d-none')
        $('.detail-request-block').addClass('d-none')
    }
}

var setActiveDiscount = function (target) {
    var input = target.find('.custom-control-input')

    if (input.is(':checked')) {
        input.attr('checked', false)
    } else {
        input.attr('checked', true)
    }
    calculateDiscount()
}

var calculateDiscount = function () {
    var discountItems = $('.element-discount-item input:checked')
    var price = cartChoice.price
    cartChoice.discountPercent = 0
    cartChoice.discountSumm = 0

    discountItems.each(function (i, el) {
        var discount = $(el).closest('.element-discount-item').data('value')
        var discountSumm = Math.round(cartChoice.price / 100 * discount)

        if (discountSumm) {
            price = price - discountSumm
            cartChoice.discountPercent += parseInt(discount)
            cartChoice.discountSumm += parseInt(discountSumm)
        }
    })
    cartChoice.priceFinal = price
    $('.catalog-element-summ').text(BX.Currency.currencyFormat(cartChoice.priceFinal, "RUB", 1))
}

var calculateFence = function () {
    var lenght = $('.calculator-lenght').val(),
        width = $('.calculator-width').val(),
        pMetr = 1, price = 0,
        button = $('.js-calculate-fence')

    if (lenght && width) {
        pMetr = ((lenght * 2) + (width * 2)) / 100;
        price = pMetr * cartChoice.productPrice
    }

    cartChoice.props[button.data('title')] = {
        title: button.data('title'),
        value: lenght + 'x' + width + '=' + pMetr + 'п.м',
        price: Math.round(price),
        code: button.data('code')
    }
    cartChoice.prices[button.data('code')] = Math.round(price)
    cartChoice.pMetr = pMetr

    $('.calculator-result').text('= ' + pMetr + ' п.м')

    // установка
    var installBlock = $('.element-install_item.install-yes')
    var installPrice = pMetr * installBlock.attr('data-priceone')
    installBlock.attr('data-price', installPrice)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(installPrice, "RUB", 1))
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = installPrice
        cartChoice.props['install']['price'] = installPrice
    }
    cartChoice.fenceInstall = installPrice

    //бетонный цоколь
    $('.element-tsokol_item').each(function (i, el) {
        var tsokolPriceOne = parseInt($(el).attr('data-priceone')),
            tsokolInstall = parseInt($(el).attr('data-install'))
        var tsokolPriceSumm = (pMetr * tsokolPriceOne) + (pMetr * tsokolInstall)

        $(el).attr('data-price', Math.round(tsokolPriceSumm))
            .find('.element-price').text(BX.Currency.currencyFormat(Math.round(tsokolPriceSumm), "RUB", 1))

        setActiveDecor($('.element-tsokol_item.active'))
    })
    if (cartChoice.prices['tsokol']) {
        var activeItem = $('.element-tsokol_item.active'),
            activeItemPrice = (pMetr * activeItem.data('priceone')) + (pMetr * activeItem.data('install'))
        cartChoice.prices['tsokol'] = activeItemPrice
        cartChoice.props['tsokol']['price'] = Math.round(activeItemPrice)
    }

    //парапет на бетонный цоколь
    $('.element-parapet_item').each(function (i, el) {
        var parapetPriceOne = parseInt($(el).attr('data-priceone')),
            parapetInstall = parseInt($(el).attr('data-install'))
        var parapetPriceSumm = (pMetr * parapetPriceOne) + (pMetr * parapetInstall)

        $(el).attr('data-price', Math.round(parapetPriceSumm))
            .find('.element-price').text(BX.Currency.currencyFormat(Math.round(parapetPriceSumm), "RUB", 1))

        setActiveDecor($('.element-parapet_item.active'))
    })
    if (cartChoice.prices['parapet']) {
        var activeItem = $('.element-parapet_item.active'),
            activeItemPrice = (pMetr * activeItem.data('priceone')) + (pMetr * activeItem.data('install'))
        cartChoice.prices['parapet'] = activeItemPrice
        cartChoice.props['parapet']['price'] = Math.round(activeItemPrice)
    }

    //покраска
    $('.element-painting_item').each(function (i, el) {
        var paintingPriceOne = parseInt($(el).attr('data-priceone')),
            paintingInstall = parseInt($(el).attr('data-install'))
        var paintingPriceSumm = (pMetr * paintingPriceOne) + (pMetr * paintingInstall)

        $(el).attr('data-price', Math.round(paintingPriceSumm))
            .find('.element-price').text(BX.Currency.currencyFormat(Math.round(paintingPriceSumm), "RUB", 1))

        setActiveDecor($('.element-painting_item.active'))
    })
    if (cartChoice.prices['painting']) {
        var activeItem = $('.element-painting_item.active'),
            activeItemPrice = (pMetr * activeItem.data('priceone')) + (pMetr * activeItem.data('install'))
        cartChoice.prices['painting'] = activeItemPrice
        cartChoice.props['painting']['price'] = Math.round(activeItemPrice)
    }

    calculatePrice()
    buildChoice()
}

/**
    *   Расчет оформления буквами в разделе Бронзовые буквы
    */
var calculateLetters = function () {
    var surnameInput = $('.letter-text_surname'),
        nameInput = $('.letter-text_firstname'),
        patronymicInput = $('.letter-text_patronymic'),
        datesInput = $('.letter-text_dates'),

        //тексты без пробелов
        surnameCleanText = surnameInput.val().replace(/\s+/g, ''),
        nameCleanText = nameInput.val().replace(/\s+/g, ''),
        patronymicCleanText = patronymicInput.val().replace(/\s+/g, ''),
        datesCleanText = datesInput.val().replace(/\s+/g, ''),
        allText = surnameCleanText + nameCleanText + patronymicCleanText + datesCleanText,

        lettersCount = 0,
        upperCount = 0,
        lowerCount = 0,
        digitCount = 0,
        symbolsCount = 0,

        upperPrice = parseInt($('.upper-size-item.active').data('price')),
        lowerPrice = parseInt($('.lower-size-item.active').data('price')),
        digitPrice = parseInt($('.digit-size-item.active').data('price')),
        symbolsPrice = parseInt($('.digit-size-item.active').data('price'))

        if (cartChoice.letters.upperPrice) {
            upperPrice = cartChoice.letters.upperPrice
            lowerPrice = cartChoice.letters.lowerPrice
            digitPrice = cartChoice.letters.digitPrice
            symbolsPrice = cartChoice.letters.symbolsPrice
        }

    if (cartChoice.letters.dotsPrice) {
        symbolsPrice = cartChoice.letters.dotsPrice
    }

    if (allText.length > 0) {
        lettersCount = parseInt(allText.length)

        var mUp = allText.match(/[A-ZА-ЯЁ]/g),
            mLo = allText.match(/[a-zа-яё]/g),
            mDi = allText.match(/[0-9]/g)

        upperCount = mUp ? mUp.length : 0
        lowerCount = mLo ? mLo.length : 0
        digitCount = mDi ? mDi.length : 0

        symbolsCount = lettersCount - (upperCount + lowerCount + digitCount)
    }

    var sumUpper = upperCount * upperPrice,
        sumLower = lowerCount * lowerPrice,
        sumDigit = digitCount * digitPrice,
        sumSymbols = symbolsCount * symbolsPrice,
        sumAll = sumUpper + sumLower + sumDigit + sumSymbols

    var resultTextVals = []
    $('.letter-text_input').each(function (ind, el) {
        var val = $(el).val()
        if (val.length > 0) {
            resultTextVals.push(val)
        }
    })
    var resultText = resultTextVals.join('<br>')

    cartChoice.props['letters_text'] = {
        title: 'Текст',
        code: 'letters_text',
        value: resultText,
        price: null
    }
    cartChoice.props['letters_count'] = {
        title: 'Кол-во символов',
        code: 'letters_text',
        value: lettersCount,
        price: sumAll
    }
    cartChoice.prices['letters'] = sumAll

    calculateLettersInstall()
}

var calculateLettersInstall = function () {
    var installBlock = $('.element-install_item.install-yes')
    var installPrice = cartChoice.props.letters_count.value * installBlock.attr('data-priceone')
    installBlock.attr('data-price', installPrice)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(installPrice, "RUB", 1))
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = installPrice
        cartChoice.props['install']['price'] = installPrice
    }
}

$('body').on('input', '.letter-text_surname, .letter-text_firstname, .letter-text_patronymic, .letter-text_dates', function (e) {
    calculateLetters()
    calculatePrice()
    buildChoice()
})

var calculateArea = function (metr = false) {
    var lenght = $('.calculator-lenght').val(),
        width = $('.calculator-width').val(),
        area = 1, price = 0,
        button = $('.js-calculate-area'),
        suffix = ' см<sup>2</sup>',
        extra = parseInt(button.attr('data-areaextra')),
        minArea = parseInt(button.attr('data-minarea'))

    if (!lenght || !width) {
        return false
    }

    if (metr) {
        area = (lenght * width) / 10000
        suffix = ' м<sup>2</sup>'
    } else {
        area = lenght * width
    }
    price = area * cartChoice.productPrice

    if (minArea && extra) {
        if (area < minArea) {
            var priceExtra = price / 100 * extra
            price = price + priceExtra
            // price = price * 1.2
        }
    }

    // console.log(metr, area, price, priceExtra)

    cartChoice.props[button.data('code')] = {
        title: button.data('title'),
        value: lenght + 'x' + width + '=' + area + suffix,
        price: Math.round(price),
        code: button.data('code')
    }
    cartChoice.prices[button.data('code')] = Math.round(price)
    cartChoice.areaPrice = Math.round(price)
    cartChoice.area = area

    $('.calculator-result').html('= ' + area + suffix)

    if (cartChoice.prices['print200']) {
        var ratio = parseFloat($('.photoprint-print200').data('ratio'))
        cartChoice.prices['print200'] = ratio * cartChoice.areaPrice
        cartChoice.props['print200']['price'] = ratio * cartChoice.areaPrice
    }
    if (cartChoice.prices['printFigure']) {
        var ratio = parseFloat($('.photoprint-printFigure').data('ratio'))
        cartChoice.prices['printFigure'] = ratio * cartChoice.areaPrice
        cartChoice.props['printFigure']['price'] = ratio * cartChoice.areaPrice
    }

    if (metr) {
        calculateAreaMertInstall()
        calculateAreaMertBeton()
    }
    calculatePrice()
    buildChoice()
}

var calculateAreaMertInstall = function () {
    var installBlock = $('.element-install_item.install-yes')
    var installPrice = cartChoice.area * installBlock.attr('data-priceone')
    installBlock.attr('data-price', installPrice)
        .find('.catalog-install_value').text(BX.Currency.currencyFormat(installPrice, "RUB", 1))
    if (cartChoice.prices['install']) {
        cartChoice.prices['install'] = installPrice
        cartChoice.props['install']['price'] = installPrice
    }
}

var calculateAreaMertBeton = function () {
    var betonBlock = $('.element-beton_item.beton-yes')
    var betonPrice = cartChoice.area * betonBlock.attr('data-priceone')
    betonBlock.attr('data-price', betonPrice)
        .find('.catalog-beton_value').text(BX.Currency.currencyFormat(betonPrice, "RUB", 1))
    if (cartChoice.prices['beton_base']) {
        cartChoice.prices['beton_base'] = betonPrice
        cartChoice.props['beton_base']['price'] = betonPrice
    }
}

var setActivePhotoprintService = function (target) {

    var selRatio = parseFloat(target.data('ratio')),
        selPrice = parseFloat(target.data('price')),
        selPriceMeasure = parseFloat(target.data('priceMeasure'))

    if (target.is(':checked')) {
        if (selRatio > 0) {
            addPrice = cartChoice.areaPrice * selRatio
        } else if (selPrice > 0) {
            addPrice = selPrice
        } else if (selPriceMeasure > 0) {
            addPrice = cartChoice.area * selPriceMeasure
        }
        cartChoice.props[target.data('code')] = {
            title: '',
            value: target.data('title'),
            code: target.data('code'),
            price: addPrice
        }
        cartChoice.prices[target.data('code')] = addPrice
    } else {
        delete cartChoice.props[target.data('code')]
        delete cartChoice.prices[target.data('code')]
    }

    calculatePrice()
    buildChoice()
}
/* End */
;
; /* Start:"a:4:{s:4:"full";s:63:"/bitrix/components/bitrix/search.title/script.js?16703251249847";s:6:"source";s:48:"/bitrix/components/bitrix/search.title/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
function JCTitleSearch(arParams)
{
	var _this = this;

	this.arParams = {
		'AJAX_PAGE': arParams.AJAX_PAGE,
		'CONTAINER_ID': arParams.CONTAINER_ID,
		'INPUT_ID': arParams.INPUT_ID,
		'MIN_QUERY_LEN': parseInt(arParams.MIN_QUERY_LEN)
	};
	if(arParams.WAIT_IMAGE)
		this.arParams.WAIT_IMAGE = arParams.WAIT_IMAGE;
	if(arParams.MIN_QUERY_LEN <= 0)
		arParams.MIN_QUERY_LEN = 1;

	this.cache = [];
	this.cache_key = null;

	this.startText = '';
	this.running = false;
	this.runningCall = false;
	this.currentRow = -1;
	this.RESULT = null;
	this.CONTAINER = null;
	this.INPUT = null;
	this.WAIT = null;

	this.ShowResult = function(result)
	{
		if(BX.type.isString(result))
		{
			_this.RESULT.innerHTML = result;
		}

		_this.RESULT.style.display = _this.RESULT.innerHTML !== '' ? 'block' : 'none';
		var pos = _this.adjustResultNode();

		//adjust left column to be an outline
		var res_pos;
		var th;
		var tbl = BX.findChild(_this.RESULT, {'tag':'table','class':'title-search-result'}, true);
		if(tbl)
		{
			th = BX.findChild(tbl, {'tag':'th'}, true);
		}

		if(th)
		{
			var tbl_pos = BX.pos(tbl);
			tbl_pos.width = tbl_pos.right - tbl_pos.left;

			var th_pos = BX.pos(th);
			th_pos.width = th_pos.right - th_pos.left;
			th.style.width = th_pos.width + 'px';

			_this.RESULT.style.width = (pos.width + th_pos.width) + 'px';

			//Move table to left by width of the first column
			_this.RESULT.style.left = (pos.left - th_pos.width - 1)+ 'px';

			//Shrink table when it's too wide
			if((tbl_pos.width - th_pos.width) > pos.width)
				_this.RESULT.style.width = (pos.width + th_pos.width -1) + 'px';

			//Check if table is too wide and shrink result div to it's width
			tbl_pos = BX.pos(tbl);
			res_pos = BX.pos(_this.RESULT);
			if(res_pos.right > tbl_pos.right)
			{
				_this.RESULT.style.width = (tbl_pos.right - tbl_pos.left) + 'px';
			}
		}

		var fade;
		if(tbl) fade = BX.findChild(_this.RESULT, {'class':'title-search-fader'}, true);
		if(fade && th)
		{
			res_pos = BX.pos(_this.RESULT);
			fade.style.left = (res_pos.right - res_pos.left - 18) + 'px';
			fade.style.width = 18 + 'px';
			fade.style.top = 0 + 'px';
			fade.style.height = (res_pos.bottom - res_pos.top) + 'px';
			fade.style.display = 'block';
		}
	};

	this.onKeyPress = function(keyCode)
	{
		var tbl = BX.findChild(_this.RESULT, {'tag':'table','class':'title-search-result'}, true);
		if(!tbl)
			return false;

		var i;
		var cnt = tbl.rows.length;

		switch (keyCode)
		{
		case 27: // escape key - close search div
			_this.RESULT.style.display = 'none';
			_this.currentRow = -1;
			_this.UnSelectAll();
		return true;

		case 40: // down key - navigate down on search results
			if(_this.RESULT.style.display == 'none')
				_this.RESULT.style.display = 'block';

			var first = -1;
			for(i = 0; i < cnt; i++)
			{
				if(!BX.findChild(tbl.rows[i], {'class':'title-search-separator'}, true))
				{
					if(first == -1)
						first = i;

					if(_this.currentRow < i)
					{
						_this.currentRow = i;
						break;
					}
					else if(tbl.rows[i].className == 'title-search-selected')
					{
						tbl.rows[i].className = '';
					}
				}
			}

			if(i == cnt && _this.currentRow != i)
				_this.currentRow = first;

			tbl.rows[_this.currentRow].className = 'title-search-selected';
		return true;

		case 38: // up key - navigate up on search results
			if(_this.RESULT.style.display == 'none')
				_this.RESULT.style.display = 'block';

			var last = -1;
			for(i = cnt-1; i >= 0; i--)
			{
				if(!BX.findChild(tbl.rows[i], {'class':'title-search-separator'}, true))
				{
					if(last == -1)
						last = i;

					if(_this.currentRow > i)
					{
						_this.currentRow = i;
						break;
					}
					else if(tbl.rows[i].className == 'title-search-selected')
					{
						tbl.rows[i].className = '';
					}
				}
			}

			if(i < 0 && _this.currentRow != i)
				_this.currentRow = last;

			tbl.rows[_this.currentRow].className = 'title-search-selected';
		return true;

		case 13: // enter key - choose current search result
			if(_this.RESULT.style.display == 'block')
			{
				for(i = 0; i < cnt; i++)
				{
					if(_this.currentRow == i)
					{
						if(!BX.findChild(tbl.rows[i], {'class':'title-search-separator'}, true))
						{
							var a = BX.findChild(tbl.rows[i], {'tag':'a'}, true);
							if(a)
							{
								window.location = a.href;
								return true;
							}
						}
					}
				}
			}
		return false;
		}

		return false;
	};

	this.onTimeout = function()
	{
		_this.onChange(function(){
			setTimeout(_this.onTimeout, 500);
		});
	};

	this.onChange = function(callback)
	{
		if (_this.running)
		{
			_this.runningCall = true;
			return;
		}
		_this.running = true;

		if(_this.INPUT.value != _this.oldValue && _this.INPUT.value != _this.startText)
		{
			_this.oldValue = _this.INPUT.value;
			if(_this.INPUT.value.length >= _this.arParams.MIN_QUERY_LEN)
			{
				_this.cache_key = _this.arParams.INPUT_ID + '|' + _this.INPUT.value;
				if(_this.cache[_this.cache_key] == null)
				{
					if(_this.WAIT)
					{
						var pos = BX.pos(_this.INPUT);
						var height = (pos.bottom - pos.top)-2;
						_this.WAIT.style.top = (pos.top+1) + 'px';
						_this.WAIT.style.height = height + 'px';
						_this.WAIT.style.width = height + 'px';
						_this.WAIT.style.left = (pos.right - height + 2) + 'px';
						_this.WAIT.style.display = 'block';
					}

					BX.ajax.post(
						_this.arParams.AJAX_PAGE,
						{
							'ajax_call':'y',
							'INPUT_ID':_this.arParams.INPUT_ID,
							'q':_this.INPUT.value,
							'l':_this.arParams.MIN_QUERY_LEN
						},
						function(result)
						{
							_this.cache[_this.cache_key] = result;
							_this.ShowResult(result);
							_this.currentRow = -1;
							_this.EnableMouseEvents();
							if(_this.WAIT)
								_this.WAIT.style.display = 'none';
							if (!!callback)
								callback();
							_this.running = false;
							if (_this.runningCall)
							{
								_this.runningCall = false;
								_this.onChange();
							}
						}
					);
					return;
				}
				else
				{
					_this.ShowResult(_this.cache[_this.cache_key]);
					_this.currentRow = -1;
					_this.EnableMouseEvents();
				}
			}
			else
			{
				_this.RESULT.style.display = 'none';
				_this.currentRow = -1;
				_this.UnSelectAll();
			}
		}
		if (!!callback)
			callback();
		_this.running = false;
	};

	this.onScroll = function ()
	{
		if(BX.type.isElementNode(_this.RESULT)
			&& _this.RESULT.style.display !== "none"
			&& _this.RESULT.innerHTML !== ''
		)
		{
			_this.adjustResultNode();
		}
	};

	this.UnSelectAll = function()
	{
		var tbl = BX.findChild(_this.RESULT, {'tag':'table','class':'title-search-result'}, true);
		if(tbl)
		{
			var cnt = tbl.rows.length;
			for(var i = 0; i < cnt; i++)
				tbl.rows[i].className = '';
		}
	};

	this.EnableMouseEvents = function()
	{
		var tbl = BX.findChild(_this.RESULT, {'tag':'table','class':'title-search-result'}, true);
		if(tbl)
		{
			var cnt = tbl.rows.length;
			for(var i = 0; i < cnt; i++)
				if(!BX.findChild(tbl.rows[i], {'class':'title-search-separator'}, true))
				{
					tbl.rows[i].id = 'row_' + i;
					tbl.rows[i].onmouseover = function (e) {
						if(_this.currentRow != this.id.substr(4))
						{
							_this.UnSelectAll();
							this.className = 'title-search-selected';
							_this.currentRow = this.id.substr(4);
						}
					};
					tbl.rows[i].onmouseout = function (e) {
						this.className = '';
						_this.currentRow = -1;
					};
				}
		}
	};

	this.onFocusLost = function(hide)
	{
		setTimeout(function(){_this.RESULT.style.display = 'none';}, 250);
	};

	this.onFocusGain = function()
	{
		if(_this.RESULT.innerHTML.length)
			_this.ShowResult();
	};

	this.onKeyDown = function(e)
	{
		if(!e)
			e = window.event;

		if (_this.RESULT.style.display == 'block')
		{
			if(_this.onKeyPress(e.keyCode))
				return BX.PreventDefault(e);
		}
	};

	this.adjustResultNode = function()
	{
		if(!(BX.type.isElementNode(_this.RESULT)
			&& BX.type.isElementNode(_this.CONTAINER))
		)
		{
			return { top: 0, right: 0, bottom: 0, left: 0, width: 0, height: 0 };
		}

		var pos = BX.pos(_this.CONTAINER);

		_this.RESULT.style.position = 'absolute';
		_this.RESULT.style.top = (pos.bottom + 2) + 'px';
		_this.RESULT.style.left = pos.left + 'px';
		_this.RESULT.style.width = pos.width + 'px';

		return pos;
	};

	this._onContainerLayoutChange = function()
	{
		if(BX.type.isElementNode(_this.RESULT)
			&& _this.RESULT.style.display !== "none"
			&& _this.RESULT.innerHTML !== ''
		)
		{
			_this.adjustResultNode();
		}
	};
	this.Init = function()
	{
		this.CONTAINER = document.getElementById(this.arParams.CONTAINER_ID);
		BX.addCustomEvent(this.CONTAINER, "OnNodeLayoutChange", this._onContainerLayoutChange);

		this.RESULT = document.body.appendChild(document.createElement("DIV"));
		this.RESULT.className = 'title-search-result';
		this.INPUT = document.getElementById(this.arParams.INPUT_ID);
		this.startText = this.oldValue = this.INPUT.value;
		BX.bind(this.INPUT, 'focus', function() {_this.onFocusGain()});
		BX.bind(this.INPUT, 'blur', function() {_this.onFocusLost()});
		this.INPUT.onkeydown = this.onKeyDown;

		if(this.arParams.WAIT_IMAGE)
		{
			this.WAIT = document.body.appendChild(document.createElement("DIV"));
			this.WAIT.style.backgroundImage = "url('" + this.arParams.WAIT_IMAGE + "')";
			if(!BX.browser.IsIE())
				this.WAIT.style.backgroundRepeat = 'none';
			this.WAIT.style.display = 'none';
			this.WAIT.style.position = 'absolute';
			this.WAIT.style.zIndex = '1100';
		}

		BX.bind(this.INPUT, 'bxchange', function() {_this.onChange()});

		var fixedParent = BX.findParent(this.CONTAINER, BX.is_fixed);
		if(BX.type.isElementNode(fixedParent))
		{
			BX.bind(window, 'scroll', BX.throttle(this.onScroll, 100, this));
		}
	};
	BX.ready(function (){_this.Init(arParams)});
}

/* End */
