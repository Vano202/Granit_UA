$(document).ready(function () {
    $('.input_phone').inputmask("+7(999) 999-99-99")

    $('.main-slider_wrap').slick({
        infinite: true,
        dots: false,
        prevArrow: ".main-slider-prev",
        nextArrow: ".main-slider-next",
    })
    $('.main-texts_wrap').slick({
        infinite: true,
        dots: false,
    })

    // $('.top-catalog_button').on('click', function (e) {
    //     e.preventDefault()
    //     $('.top-catalog_wrap').addClass('open')
    //     $('body').toggleClass('overflow-hidden')
    // })

    // $('.top-catalog-close').on('click', function (e) {
    //     closeTopCatalog()
