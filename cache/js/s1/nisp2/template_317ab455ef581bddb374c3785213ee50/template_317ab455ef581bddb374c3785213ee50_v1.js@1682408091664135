$('body').on('click', '.mobile-top-catalog_menu-link', function (e) {
    var thisItem = $(this).closest('.mobile-top-catalog_menu-item')
    if (thisItem.hasClass('parent')) {
        e.preventDefault()
        $('.mobile-top-catalog_submenu').removeClass('open')
        thisItem.find('.mobile-top-catalog_submenu').addClass('open')
    }
})

$('body').on('click', '.mobile-top-catalog_submenu-back', function () {
    $(this).closest('.mobile-top-catalog_submenu').removeClass('open')
})

$('body').on('click', '.mobile-top-catalog_section-title', function () {
    var wrap = $(this).closest('.mobile-top-catalog_submenu-item')
    wrap.toggleClass('open')
})

$('body').on('click', '.wiki-menu .menu-item-header', function (e) {
    var _this = $(this), menuItem = _this.closest('.menu-item')
    if (menuItem.hasClass('parent')) {
        e.preventDefault()
    }
    if (menuItem.hasClass('open')) {
        menuItem.removeClass('open')
    } else {
        menuItem.addClass('open')
    }
})
