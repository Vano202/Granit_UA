    // $('.top-catalog_wrap').on('click', function (e) {
    //     if ($(e.target).hasClass('top-catalog_wrap')) {
    //         closeTopCatalog()
    //     }
    // })

    $(document).on('scroll', function (e) {
        showFixedHeader()
    })

    // var closeTopCatalog = function () {
    //     $('.top-catalog_wrap').removeClass('open')
    //     $('.submenu-wrap').empty()
    //     $('body').removeClass('overflow-hidden')
    //     $('.top-catalog-close').removeClass('sub')
    // }

    $('.scrollbar-inner').scrollbar({
        scrollX: false
    })

    // $('.top-catalog-menu_root-item').on('mouseenter', function (e) {
    //     var submenu = $(this).find('.top-catalog-submenu');
    //     $('.top-catalog-menu_root-item').removeClass('active')
    //     $('.submenu-wrap').empty()
    //     $('.top-catalog-close').removeClass('sub')
    //     if ($(this).hasClass('parent')) {
    //         $(this).addClass('active')
    //         submenu.find('.top-catalog-submenu_title').text($(this).data('title'))
    //         submenu.clone().appendTo('.submenu-wrap')
    //         $('.top-catalog-close').addClass('sub')
    //     }
    // })
    // $('.top-catalog-menu_root-link').on('mouseenter', function (e) {
    //     var item = $(this).parent('.top-catalog-menu_root-item')
    //     submenu = item.children('.top-catalog-submenu')
    //     $('.top-catalog-menu_root-item').removeClass('active')
    //     $('.submenu-wrap').empty()
    //     $('.top-catalog-close').removeClass('sub')
    //     if (item.hasClass('parent')) {
    //         item.addClass('active')
    //         submenu.find('.top-catalog-submenu_title').text($(this).data('title'))
    //         submenu.clone().appendTo('.submenu-wrap')
    //         $('.top-catalog-close').addClass('sub')
    //     }
    // })

    // $('.top-search-button').on('click', function (e) {
    //     $('.top-search').toggleClass('open')
    //     $('.top-search-input').focus()
    // })

    $('.js-open-callback-popup').on('click', function (e) {
        e.preventDefault()
        callbackOpen()
    })
    $('.callback-popup_close').on('click', function (e) {
        callbackClose()
    })

    $('.callback-popup_form').on('submit', function (e) {
        e.preventDefault()
        var _self = $(this)
        var resultBlock = _self.closest('.callback-popup_window').find('.popup-result')
        $.ajax({
            method: 'post',
            url: '/ajax/callback.php',
            dataType: 'html',
            data: _self.serialize(),
            success: function (resp) {
                // _self.closest('.callback-popup_window').html(resp)
                if (resp.success) {
                    resultBlock.removeClass('d-none')
                    ym(40267199,'reachGoal','sendCallMessage')
                } else {
                    resultBlock.find('.popup-result_body').text(resp.message)
                    resultBlock.removeClass('d-none')
                }
            }
        })
    })
    $('.js-show-sort').on('click', function (e) {
        $('.catalog-sort_menu').toggleClass('d-none')
    })

    $('.js-show-filter').on('click', function (e) {
        e.preventDefault()
        $('.catalog-filter').toggleClass('open')
        $('body').toggleClass('overflow-hidden')
    })

    $('.catalog-filter-close').on('click', function (e) {
        $('.catalog-filter').removeClass('open')
        $('body').removeClass('overflow-hidden')
    })
    $('.carousel-list').slick({
        slidesToShow: 4,
        dots: false,
        // arrows: false,
        prevArrow: '.carousel-prev',
        nextArrow: '.carousel-next',
        responsive: [
            {
                breakpoint: 1280,
                settings: {
                    slidesToShow: 4
                }
            },
            {
                breakpoint: 950,
                settings: {
                    slidesToShow: 2
                }
            }
        ]
    })

    $('.left-menu a').on('click', function () {
        $('.left-menu a').removeClass('active')
        $(this).addClass('active')
    })

    $('.contacts-tabs .tab-link').on('click', function (e) {
        $('.contacts-tabs .tab-link').removeClass('active')
        $(this).addClass('active')
        $('.contacts-tabs .tab-item').removeClass('active')
        $('#' + $(this).data('target')).addClass('active')
    })

    $('body').on('click', '.theme-switcher', function () {
        $(this).toggleClass('purple')
        var _self = $(this)
        $.ajax({
            method: 'post',
            url: '/ajax/switcher.php',
            dataType: 'json',
            data: { action: 'changeTheme' },
            success: function (resp) {
                if (resp.success) {
                    window.location.reload()
                }
            }
        })
    })

    $('body').on('submit', '.bottom-cta_form', function (e) {
        e.preventDefault()
        var _self = $(this)
        var resultBlock = _self.closest('.bottom-cta').find('.bottom-cta_result')
        $.ajax({
            method: 'post',
            url: '/ajax/callback.php',
            dataType: 'html',
            data: _self.serialize(),
            success: function (resp) {
                _self.addClass('d-none')
                if (resp.success) {
                    resultBlock.removeClass('d-none')
                    ym(40267199,'reachGoal','callMeManager')
                } else {
                    resultBlock.find('.result-text').text(resp.message)
                    resultBlock.removeClass('d-none')
                }
            }
        })
    })

    var leftMenu = $('.left-menu'),
        menuItems = leftMenu.find('a'),
        leftMenuHeight = leftMenu.outerHeight() + 150
    menuItems.click(function (e) {
        var href = $(this).attr("href"),
            leftMenuHeight = leftMenu.outerHeight() + 20,
            offsetTop = href === "#" ? 0 : $(href).offset().top - 90;
        $('html, body').stop().animate({
            scrollTop: offsetTop
        }, 300);
        e.preventDefault();
    });
    // on document load

}) // document ready

var callbackOpen = function (message = false) {
    $('body').addClass('overflow-hidden')
    $('.callback-popup').removeClass('d-none')
    if (message) {
        // console.log(message)
        $('.callback-popup_form').append('<input type="hidden" name="message" value="' + message + '">')
    }
    ym(40267199,'reachGoal','clickToCall')
}
var callbackClose = function () {
    $('.callback-popup').addClass('d-none')
    $('body').removeClass('overflow-hidden')
    $('.popup-result').addClass('d-none')
    $('.callback-popup_form [name="message"]').remove()
}

/** Быстрый заказ */
$('body').on('click', '.js-oneclick-button', function () {
    oneclickOpen()
    // console.log(cartChoice)
})
$('body').on('click', '.oneclick-popup_close', function () {
    oneclickClose()
    $('.oneclick-datainput').remove()
})

var oneclickOpen = function () {
    $('body').addClass('overflow-hidden')
    $('.oneclick-popup').removeClass('d-none')
    // $('.oneclick-popup_submit').append('<input type="hidden" name="data" value="'+JSON.stringify(cartChoice.props)+'">')
    var inputsWrap = $('.oneclick-popup_submit')
    for (key in cartChoice.props) {
        var item = cartChoice.props[key]
        if (item.value) {
            inputsWrap.append('<input class="oneclick-datainput" type="hidden" name="data['+key+'][name]" value="'+item.title+'">')
            inputsWrap.append('<input class="oneclick-datainput" type="hidden" name="data['+key+'][value]" value="'+item.value+'">')
        }
        if (item.price) {
            inputsWrap.append('<input class="oneclick-datainput" type="hidden" name="data['+key+'][price]" value="'+item.price+'">')
        }
    }
    inputsWrap.append('<input class="oneclick-datainput" type="hidden" name="price" value="'+cartChoice.priceFinal+'">')
    inputsWrap.append('<input class="oneclick-datainput" type="hidden" name="product" value="'+cartChoice.name+'">')
    ym(40267199,'reachGoal','clickToQuickOrder')
}
var oneclickClose = function () {
    $('.oneclick-popup').addClass('d-none')
    $('body').removeClass('overflow-hidden')
    $('.popup-result').addClass('d-none')
}
$('.oneclick-popup_form').on('submit', function (e) {
    e.preventDefault()
    var _self = $(this)
    var resultBlock = _self.closest('.oneclick-popup_window').find('.popup-result')
    $.ajax({
        method: 'post',
        url: '/ajax/oneclick.php',
        dataType: 'json',
        data: _self.serialize(),
        success: function (resp) {
            if (resp.success) {
                resultBlock.removeClass('d-none')
                ym(40267199,'reachGoal','sendQuickOrder')
            } else {
                resultBlock.find('.popup-result_body').text(resp.message)
                resultBlock.removeClass('d-none')
            }
        }
    })
})

// -----------------------------
$(document).ready(function () {
    updateTopCartCount()
    showFixedHeader()
    topFavoriteCount()
})

var showFixedHeader = function () {
    var mainHeader = $('.site-header'),
        documentTop = $(document).scrollTop(),
        // point = $('.header-main-line').offset().top + $('.header-main-line').outerHeight()
        point = 153

    if (documentTop >= point) {
        mainHeader.addClass('fixed')
        $('body').addClass('fixed-header')
    } else {
        mainHeader.removeClass('fixed')
        $('body').removeClass('fixed-header')
    }
}

/* TOP CART */
$('body').on('click', '.to-cart-button', function (e) {
    var _self = $(this)
    if (_self.hasClass('disabled')) {
        return;
    }
    // var dataObject = cartChoice
    // dataObject['action'] = 'add'
    _self.attr('disable', true)
    $.ajax({
        method: 'post',
        url: '/ajax/cart.php',
        dataType: 'json',
        data: { action: 'add', data: cartChoice },
        success: function (resp) {
            // console.log(resp)
            _self.attr('disable', false)
            topCartOpen()
            updateTopCartCount()
            ym(40267199,'reachGoal','addToCart');
        }
    })
})
$('body').on('click', '.js-open-top-cart', function (e) {
    e.preventDefault()
    topCartOpen()
})

$('body').on('click', '.top-cart_close', function (e) {
    e.preventDefault()
    topCartClose()
})

var topCartOpen = function () {
    $('.top-cart_popup').addClass('open')
    topCartUpdate()
}

var topCartClose = function () {
    $('.top-cart_popup').removeClass('open')
}

var topCartUpdate = function () {
    var wrapper = $('.top-cart_wrapper')
    wrapper.addClass('preload')
    $.ajax({
        method: 'post',
        url: '/ajax/cart.php',
        dataType: 'html',
        data: { action: 'updateTop' },
        success: function (resp) {
            // console.log(resp)
            wrapper.html(resp)
                .removeClass('preload')
            updateTopCartCount()
        }
    })
}

var topCartQuantity = function (cartId, quantity) {
    $.ajax({
        method: 'post',
        url: '/ajax/cart.php',
        dataType: 'json',
        data: { action: 'quantity', id: cartId, quantity: quantity },
        success: function (resp) {
            // console.log(resp)
            topCartUpdate()
        }
    })
}

// var topCartDelete = function (cartId) {

//     topCartUpdate()
// }

/* top search */
var topSearchOpen = function () {
    $('.searchtitle').addClass('active')
    $('body').append('<div class="popup-overlay search"></div>')
        // .addClass('popup')
}
var topSearchClose = function () {
    $('.searchtitle').removeClass('active')
    $('.popup-overlay').remove()
    // $('body').removeClass('popup')
}
$('.top-search-button').on('click', function (e) {
    e.preventDefault()
    topSearchOpen()
})
$('.top-search-close').on('click', function (e) {
    topSearchClose()
})
$('body').on('click', function (e) {
    if ($('.popup-overlay').length) {
        if ($(e.target).closest('.searchtitle').length == 0 && $(e.target).closest('.top-search-button').length == 0) {
            topSearchClose()
        }
    }
})

var updateTopCartCount = function () {
    $.ajax({
        method: 'post',
        url: '/ajax/cart.php',
        dataType: 'json',
        data: { action: 'count' },
        success: function (resp) {
            // console.log(resp)
            $('.top-cart_count').text(resp.count)
        }
    })
}

$('body').on('click', '.top-cart-quantity .quantity-control_plus', function () {
    var input = $(this).closest('.quantity-control').find('.quantity-control_input'),
        inputVal = input.val()
    input.val(parseInt(inputVal) + 1)
    topCartQuantity(input.data('id'), input.val())
})
$('body').on('click', '.top-cart-quantity .quantity-control_minus', function () {
    var input = $(this).closest('.quantity-control').find('.quantity-control_input'),
        inputVal = input.val()
    input.val(parseInt(inputVal) - 1)
    if (parseInt(input.val()) < 1) {
        input.val(1)
    }
    topCartQuantity(input.data('id'), input.val())
})
$('body').on('change keydown paste input', '.top-cart-quantity .quantity-control_input', function () {
    var value = $(this).val().replace(/[\D]+/g, '')
    // console.log(value)
    if (value < 1 || value.length == 0 || value == NaN) {
        $(this).val(1)
    } else {
        $(this).val(value)
    }
    topCartQuantity(input.data('id'), $(this).val())
})
$('body').on('click', '.top-cart_delete', function () {
    var _self = $(this)
    $.ajax({
        method: 'post',
        url: '/ajax/cart.php',
        dataType: 'json',
        data: { action: 'delete', id: _self.data('id') },
        success: function (resp) {
            // console.log(resp)
            topCartUpdate()
        }
    })
})

$('body').on('click', '.favorite-remove', function (e) {
    e.preventDefault()
    e.stopPropagation()
    var _self = $(this)
    _self.attr('disabled', true)
    $.ajax({
        method: 'post',
        url: '/ajax/favorite.php',
        dataType: 'json',
        data: { action: 'addRemove', id: _self.data('id') },
        success: function (resp) {
            if (resp.action == 'add') {
                _self.addClass('active')
            } else {
                _self.removeClass('active')
                _self.closest('.product-item-small-card').remove()
            }
            _self.attr('disabled', false)
            topFavoriteCount()
        }
    })
})

var topFavoriteCount = function () {
    $.ajax({
        method: 'post',
        url: '/ajax/favorite.php',
        dataType: 'json',
        data: {action: 'count'},
        success: function (resp) {
            // console.log('favor '+resp.count)
            $('.top-favorites_count').text(resp.count)
        }
    })
}

$('body').on('click', '.js-item-fastview', function (e) {
    if ($(window).width() <= '590') {
        return;
    }
    e.preventDefault()
    $('.fastview-popup_wrap').removeClass('d-none')
    $('body').addClass('overflow-hidden fastview')
    var _self = $(this)
    $.ajax({
        method: 'post',
        url: '/ajax/element.php',
        dataType: 'html',
        data: { act: 'fastview', id: _self.data('id') },
        success: function (resp) {
            // console.log(resp)
            $('.fastview-popup_window').html(resp)
        }
    })
})
var closeFastView = function () {
    $('.fastview-popup_wrap').addClass('d-none')
    $('body').removeClass('overflow-hidden fastview')
    $('.fastview-popup_window').empty()
}

$('body').on('click', '.fastview-popup_close', function () {
    closeFastView()
})
$('body').on('click', '.fastview-popup_wrap', function (e) {
    e.stopPropagation()
    if ($(e.target).hasClass('fastview-popup_wrap')) {
        closeFastView()
    }
})

$('body').on('click', '.photocatalog-section', function (e) {
    e.preventDefault()
    var _self = $(this),
        id = _self.data('id')

    if ($('#photocatalog-items-' + id).hasClass('loaded')) {
        $('.photocatalog-section').removeClass('active')
        _self.addClass('active')
        $('.photocatalog-items').removeClass('active')
        $('#photocatalog-items-' + id).addClass('active')
        return;
    }
    $.ajax({
        method: 'post',
        url: '',
        data: {
            action: 'photocatalogItemsLoad',
            id: id
        },
        success: function (resp) {
            $('.photocatalog-section').removeClass('active')
            _self.addClass('active')
            $('.photocatalog-items').removeClass('active')
            $('#photocatalog-items-' + id).addClass('active loaded').html(resp)
        }
    })
})

$('body').on('click', '.js-photocatalog-item', function (e) {
    e.preventDefault()
    $('.fastview-popup_wrap').removeClass('d-none')
    $('body').addClass('overflow-hidden fastview')
    var _self = $(this)
    $.ajax({
        method: 'post',
        url: '/ajax/photocatalog.php',
        dataType: 'html',
        data: { act: 'fastview', id: _self.data('id') },
        success: function (resp) {
            $('.fastview-popup_window').html(resp)
        }
    })
})

$('body').on('click', '.js-ask-price', function (e) {
    e.preventDefault()
    var dataMessage = $(this).data('message')
    callbackOpen(dataMessage)
    closeFastView()
})

